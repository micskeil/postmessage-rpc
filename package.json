{
  "name": "message-interface",
  "version": "0.1.0",
  "keywords": [
    "postMessage",
    "interface",
    "iframe"
  ],
  "homepage": "https://github.com/EDMdesigner/plugin-interface#readme",
  "bugs": {
    "url": "https://github.com/EDMdesigner/plugin-interface/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EDMdesigner/plugin-interface.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/pluginInterface.js",
      "require": "./dist/pluginInterface.cjs"
    }
  },
  "main": "./dist/pluginInterface.cjs",
  "module": "./dist/pluginInterface.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "npx eslint . --ext .js,.mjs,.cjs,.json",
    "lint-fix": "npx eslint . --ext .js,.mjs,.cjs,.json --fix",
    "prepare": "husky install",
    "preview": "vite preview",
    "test": "vitest --coverage --reporter default --run",
    "test:watch": "vitest --reporter default",
    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@vitest/coverage-istanbul": "^3.1.2",
    "@vitest/ui": "^3.1.2",
    "eslint": "^9.25.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "prettier": "3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0",
    "unplugin-auto-import": "^19.1.2",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
